{
   "kind": "Service",
   "apiVersion": "v1",
   "metadata": {
      "name": "redis-slave",
      "labels": {
         "app": "redis",
         "role": "slave"
      }
   },
   "spec": {
      "ports": [
         {
            "port": 6379,
            "targetPort": "redis-server",
            "appProtocol": "tcp"
         }
      ],
      "selector": {
         "app": "redis",
         "role": "slave"
      }
   }
}